    "playwright-qase-reporter": "^2.0.4",
    "playwright-slack-reporter": "^1.1.81",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
